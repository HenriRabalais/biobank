function formatColumn(column, cell, rowData, rowHeaders) {
    // If a column if set as hidden, don't display it
    if (loris.hiddenHeaders.indexOf(column) > -1) {
        return null;
    }

    if (column === 'Id') {
        var url = loris.BaseURL + "/biobanking/?submenu=biospecimen_collection&biospecimen_id=" + rowData[0];
        return React.createElement(
            "td",
            null,
            React.createElement(
                "a",
                { href: url },
                cell
            )
        );
    }

    if (column === 'DoB' || column == 'Collected On' || column == 'Extracted On') {
        switch(column) {
            case 'DoB'         : date = rowData[3]; break;
            case 'Collected On': date = rowData[8]; break;
            case 'Extracted On': date = rowData[9]; break;
        }

        dateText = '';
        if(date != '') {
            dateParts = new Date(date).toDateString().split(' ');
            dateText = dateParts[2] + '-' + dateParts[1] + '-' + dateParts[3];
        }

        return React.createElement(
            "td",
            null,
            dateText
        );
    }

    if (column === 'Visit') {
        var visit = rowData[5];
        if(visit === 'dna_methylation') {
            visit = 'T1';
        } else if(visit === 'dna_methylation_T2') {
            visit = 'T2';
        } else if(visit === 'dna_methylation_T3') {
            visit = 'T3';
        } else if(visit === 'dna_methylation_T4') {
            visit = 'T4';
        } else if(visit === 'MRI10years') {
            visit = 'MRI';
        } 

        var visitPscid = rowData[6];
        
        if(visitPscid != null) {
            visit = visit + " (" + visitPscid + ")";
        }
        
        return React.createElement(
            "td",
            { style: { color: 'red'}},
            visit
        );
    }

    return React.createElement(
        "td",
        null,
        cell
    );
}
